{
    "app_id": "com.mattermost.pagerduty",
    "app_type": "http",
    "version": "0_0_1",
    "display_name": "PagerDuty",
    "description": "PagerDuty cloud app for Mattermost",
    "homepage_url": "https://github.com/mattermost/mattermost-app-pagerduty",
    "root_url": "http://localhost:1337",
    "requested_permissions": [
        "act_as_user",
        "act_as_bot"
    ],
    "requested_locations": [
        "/command",
        "/channel_header",
        "/post_menu",
        "/in_post"
    ],
    "functions": [
        {
            "name": "js-function",
            "handler": "index.handler",
            "runtime": "nodejs14.x"
        }
    ],
    "assets": []
}
